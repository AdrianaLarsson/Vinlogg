<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="113dp"
    android:layout_margin="5dp"
    android:background="@color/colorMainBackground"
    android:orientation="vertical"
    android:theme="@style/Platform.MaterialComponents.Light">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="113dp"
        android:orientation="vertical">

        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="68dp">

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <TextView
                    android:id="@+id/textView18"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="5dp"
                    android:text="Årgång"
                    android:textColor="@color/colorPrimary" />

                <TextView
                    android:id="@+id/textView19"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="5dp"
                    android:text="Antal"
                    android:textColor="@color/colorPrimary" />

                <TextView
                    android:id="@+id/textView22"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="5dp"
                    android:text="Hylla"
                    android:textColor="@color/colorPrimary" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">


                <TextView
                    android:id="@+id/textViewYearDetail"
                    android:layout_width="170dp"
                    android:layout_height="70dp"
                    android:padding="5dp"
                    android:text="TextView"
                    android:textColor="@color/colorlila" />

                <TextView
                    android:id="@+id/textViewAmountDetail"
                    android:layout_width="160dp"
                    android:layout_height="70dp"
                    android:padding="5dp"
                    android:text="TextView"
                    android:textColor="@color/colorlila" />

                <TextView
                    android:id="@+id/textViewShelveDetail"
                    android:layout_width="250dp"
                    android:layout_height="70dp"
                    android:padding="5dp"
                    android:text="TextView"
                    android:textColor="@color/colorlila" />

            </TableRow>

        </TableLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp">

            <com.google.android.material.floatingactionbutton.FloatingActionButton
                android:id="@+id/floatingActionButton7"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="510dp"
                android:clickable="true"
                android:tint="@color/colorlila"
                app:backgroundTintMode="add"
                app:fabCustomSize="30dp"
                app:maxImageSize="33dp"
                app:srcCompat="@drawable/ic_icons8_plus"
                tools:ignore="VectorDrawableCompat" />

            <com.google.android.material.floatingactionbutton.FloatingActionButton
                android:id="@+id/floatingActionButton8"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:clickable="true"
                android:tint="@color/colorlila"
                app:backgroundTintMode="add"
                app:fabCustomSize="31dp"
                app:maxImageSize="31dp"
                app:srcCompat="@drawable/ic_minus"
                tools:ignore="VectorDrawableCompat" />
        </LinearLayout>

    </LinearLayout>
</androidx.cardview.widget.CardView>